<% active_table_for :types_table, @active_table_output do |t| %>
  <%- active_tr 'type', t.id do %>
    <td><%= link_to t.name, :action => 'type', :path => t.id %></td>
    <td><%= number_to_currency t.price %></td>
    <td><%= t.created_at.strftime("%m/%d/%Y") %></td>
    <td><%= t.updated_at.strftime("%m/%d/%Y") %></td>
  <% end -%>
<% end -%>
